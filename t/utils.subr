. t/libtap.subr

set -o nounset
export CITRUN_TOOLS="`pwd`/src"

strip_preamble()
{
	file="${1}"
	tail -n +35 $file.citrun > $file.citrun_nohdr
}

strip_log()
{
	sed	-e "s,^.*: ,,"	\
		-e "s,'.*','',"	\
		-e "s,(.*),()," \
		-e "/Milliseconds/d" \
		< ${1} > ${1}.stripped
}

strip_millis()
{
	ok "stripping milliseconds" sed -i -e "/Milliseconds spent/d" $1
}
